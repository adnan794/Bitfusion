package src{	import flash.display.*;	public class VideoContainer extends Sprite	{		static private  var _instance:VideoContainer;		public function VideoContainer(singletonEnforcer:SingletonEnforcer)		{		}		public static function getInstance():VideoContainer		{			if (VideoContainer._instance == null)			{				VideoContainer._instance=new VideoContainer(new SingletonEnforcer  );			}			return VideoContainer._instance;		}		public function addDisplayObject(displayObject:DisplayObject):void		{						if (numChildren > 0)			{				removeDisplayObject();				addChild(displayObject);			}			else			{				addChild(displayObject);			}		}		public function removeDisplayObject():void		{						if (numChildren > 0)			{				removeChildAt(0);							}					}					}}class SingletonEnforcer{}